{
  "name": "@visionworksco/nodejs-middleware",
  "version": "2.1.5",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "Node.js middleware",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:visionworksco/nodejs-middleware.git"
  },
  "author": "VisionWorks",
  "homepage": "https://github.com/visionworksco/nodejs-middleware",
  "bugs": {
    "url": "https://github.com/visionworksco/nodejs-middleware/issues"
  },
  "main": "dist/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=14.18.3"
  },
  "scripts": {
    "build": "npm run clean && barrelsby --config barrelsbyrc.json && tsc -p tsconfig.build.json && prettier --write ./src/index.ts",
    "clean": "rimraf dist && rimraf coverage && rimraf ./src/index.ts",
    "commit": "git add . && git commit -m 'feat: :rocket:'",
    "compile": "tsc --noEmit",
    "compile:diagnostics": "tsc --noEmit --extendedDiagnostics",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint --fix '**/*.{js,jsx,ts,tsx}'",
    "lint:license": "license-checker --production --json --out licenses.json",
    "lint:nm:audit": "npm audit",
    "lint:nm:deprecated": "./lint-nm-deprecated.sh",
    "lint:nm:unused": "./lint-nm-unused.sh",
    "lint:staged": "lint-staged",
    "npm:publish": "git push --follow-tags origin master && npm publish",
    "npm:release:first": "standard-version --first-release",
    "npm:release:major": "standard-version --release-as major",
    "npm:release:minor": "standard-version --release-as minor",
    "npm:release:patch": "standard-version --release-as patch",
    "setup": "husky install",
    "test": "npm run test:unit",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:coverage": "jest --group=unit --coverage",
    "test:integration": "jest --group=integration",
    "test:staged": "jest --onlyChanged",
    "test:unit": "jest --group=unit",
    "test:watch": "jest --group=unit --watch"
  },
  "keywords": [
    "visionworks, nodejs"
  ],
  "dependencies": {
    "amqplib": "^0.8.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/prompt-cli": "^16.0.0",
    "@types/amqplib": "^0.8.2",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/express-serve-static-core": "^4.17.21",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "27.0.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.11.2",
    "@types/pg": "^8.6.1",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "barrelsby": "^2.3.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^7.0.4",
    "jest": "^27.1.1",
    "jest-extended": "^1.2.0",
    "jest-mock-extended": "^2.0.4",
    "jest-runner-groups": "^2.1.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-package": "^1.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.1.1",
    "ts-node": "^9.0.0",
    "typescript": "4.0.3"
  },
  "types": "dist/index.d.ts"
}
